  <!-- Propuestas-->
  <section class="page-section" id="services">
    <br></br>
    <div class="container">
      <br></br>
      <div class="text-center">
        <h2 class="section-heading text-uppercase">Propuestas</h2>
        <h3 class="section-subheading text-muted">Siempre en movimiento.</h3>
      </div>
      <div class="row text-center">
        <div class="col-md-4">
          <span class="fa-stack fa-4x">
            <i class="fas fa-circle fa-stack-2x" style="color:var(--color-strong-letter)"></i>
            <i class="<%= product.icon %> "></i>
          </span>
          <h4 class="my-3"><%=product.name%></h4>
          <p class="text-muted"><%=product.description%>
            </p>
        </div>
        <div class="col-md-8">
          <div class="portfolio-item">
            <a class="portfolio-link" data-bs-toggle="modal" href="#portfolioModal1">
