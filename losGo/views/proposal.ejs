  <!-- Propuestas-->
  <section class="page-section" id="services">
    <br></br>
    <div class="container">
      <br></br>
      <div class="text-center">
        <h2 class="section-heading text-uppercase">Propuestas</h2>
        <h3 class="section-subheading text-muted">Siempre en movimiento.</h3>
      </div>
      <div class="row text-center">
        <div class="col-md-4">
          <span class="fa-stack fa-4x">
            <i class="fas fa-circle fa-stack-2x" style="color:var(--color-strong-letter)"></i>
            <i class="<%= product.icon %> "></i>
          </span>
          <h4 class="my-3"><%=product.name%></h4>
          <p class="text-muted"><%=product.description%>
            </p>
        </div>
        <div class="col-md-8">
          <div class="portfolio-item">
            <a class="portfolio-link" data-bs-toggle="modal" href="#portfolioModal1">
              <img class="img-fluid <%=product.imgclass%>" src="/images/propuesta/<%=product.mainimg%>" alt="..." />
            </a> 
          </div>
        </div>
       
      </div>
    </div>
  </section>
  <!-- Galerias-->
  <section class="page-section bg-light" id="portfolio">
    <div class="container">
      <div class="text-center">
        <!--Esto tiene que venir por parametro ya que si se apreta fooftruck trae foodtruck-->
        <h2 class="section-heading text-uppercase"><%=product.name%></h2>
      </div>
      <div class="row">
        <div class="col-lg-4 col-sm-6 mb-4">
          <!-- Portfolio item 1-->
          <div class="portfolio-item">
            <a class="portfolio-link" data-bs-toggle="modal" href="#portfolioModal1">
              <div class="portfolio-hover">
                <div class="portfolio-hover-content"><i class="fas fa-plus fa-3x"></i></div>
              </div>
              <img class="img-fluid <%=product.imgclass1%>" src="/images/propuesta/<%=product.img2%>" alt="..." />
            </a>
            <div class="portfolio-caption">
              <div class="portfolio-caption-heading">Waffle a la godoy</div>
              <div class="portfolio-caption-subheading text-muted">Delicatese</div>
            </div>
          </div>
        </div>
        <div class="col-lg-4 col-sm-6 mb-4">
          <!-- Portfolio item 2-->
